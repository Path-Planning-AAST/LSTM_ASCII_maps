    }
    bias_filler {
      type: "constant"
    }
  }
}


layer {
  name: "relu2"
  type: "ReLU"
  bottom: "fc2"
  top: "fc2"
}


layer {
  name: "fc3"
  type: "InnerProduct"
  bottom: "fc2"
  top: "fc3"
  inner_product_param {
    num_output: 16
    weight_filler {
      type: "gaussian"
	 std: 0.1
    }
    bias_filler {
      type: "constant"
    }
  }
}


layer {
  name: "relu3"
