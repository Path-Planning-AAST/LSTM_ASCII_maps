name: "LSTM_stack-drop-channels-7-128"


layer {
  name: "data"
  type: "Data"
  top: "data"
  top: "label"
  data_param {
    source: "/home/leonida/ThesisCode/NN-Roomba/SimulationEnv/Datasets/Validate_inputs-11_10240-300-2-17-21-9-lmdb/"
    batch_size: 512
    backend: LMDB
  }
 
}

layer {
	name: "Input"
	type: "Input"
	top:  "clip"
	input_param {
		shape {
			dim: 512
			dim: 11  		
		}
	
	}
	
}

layer {
  name: "data_slicer"
  type: "Slice"
  bottom: "data"
  top: "ranges"
  top: "dist_angle"
  slice_param {
    axis: 1
    slice_point: 8
  }
}

layer {
  name: "clip_slicer"
  type: "Slice"
  bottom: "clip"
  top: "clip_r"
  top: "clip_d"
  slice_param {
    axis: 1
    slice_point: 8
  }
}


layer {
  name: "lstm1_r"
  type: "LSTM"
  bottom: "ranges"
  bottom: "clip_r"
  top: "lstm1_r"
  recurrent_param {
    num_output: 32
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}



layer {
	name: "Drop_r"
	type: "Dropout"
	bottom: "lstm1_r"
	top: "lstm1_r"
	dropout_param {
		dropout_ratio: 0.5
	}
} 



layer {
  name: "lstm1_d"
  type: "LSTM"
  bottom: "dist_angle"
  bottom: "clip_d"
  top: "lstm1_d"
  recurrent_param {
    num_output: 32
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}



layer {
	name: "Drop_d"
	type: "Dropout"
	bottom: "lstm1_d"
	top: "lstm1_d"
	dropout_param {
		dropout_ratio: 0.5
	}
} 

layer {
  name: "lstm2_r"
  type: "LSTM"
  bottom: "lstm1_r"
  bottom: "clip_r"
  top: "lstm2_r"
  recurrent_param {
    num_output: 32
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}



layer {
	name: "Dropout2_r"
	type: "Dropout"
	bottom: "lstm2_r"
	top: "lstm2_r"
	dropout_param {
		dropout_ratio: 0.5
	}
} 

layer {
  name: "lstm2_d"
  type: "LSTM"
  bottom: "lstm1_d"
  bottom: "clip_d"
  top: "lstm2_d"
  recurrent_param {
    num_output: 32
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}


layer {
	name: "Dropout2_d"
	type: "Dropout"
	bottom: "lstm2_d"
	top: "lstm2_d"
	dropout_param {
		dropout_ratio: 0.5
	}
} 


layer {
  name: "concat-channels"
  bottom: "lstm2_r"
  bottom: "lstm2_d"
  top: "lstm-concat"
  type: "Concat"
  concat_param {
    axis: 1
  }
}


layer {
  name: "lstm_join"
  type: "LSTM"
  bottom: "lstm-concat"
  bottom: "clip"
  top: "lstm_join"
  recurrent_param {
    num_output: 64
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}


layer {
	name: "Drop-join"
	type: "Dropout"
	bottom: "lstm_join"
	top: "lstm_join"
	dropout_param {
		dropout_ratio: 0.5
	}
} 


layer {
  name: "out"
  type: "InnerProduct"
  bottom: "lstm_join"
  top: "out"

  inner_product_param {
    num_output: 8
    weight_filler {
      type: "gaussian"
      std: 0.1
    }
    bias_filler {
      type: "constant"
    }
  }
}

layer {
  name: "softmax"
  type: "Softmax"
  bottom: "out"
  top: "softmax"
}


layer {
  name: "argmax"
  type: "ArgMax"
  bottom: "softmax"
  top: "argmax"
}

layer {
  name: "accuracy"
  type: "Accuracy"
  bottom: "softmax"
  bottom: "label"
  top: "accuracy"  
}



